// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

//menu
model menu {
  id Int @id@default(autoincrement())
  cakeImg String
  cakeName String
  price String
}

//order form
model Consument  {
  id Int @id@default(autoincrement())
  cake String
  name String
  email String
  telpNo String
}


